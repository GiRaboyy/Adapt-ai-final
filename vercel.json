{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "functions": {
    "api/**/*.py": {
      "includeFiles": "api/**",
      "excludeFiles": "{.next,node_modules,public,app,components,lib,hooks,.git,.venv,venv}/**"
    }
  },
  "rewrites": [
    { "source": "/api/:path*", "destination": "/api/index.py" }
  ]
}
